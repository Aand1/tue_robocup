***************
**** EGPSR ****
***************

It is not possible to say every sentence. All the possibilities are defined here.
---------------------------------------------------------------------------------

<transportCmd> = 	 <transportAction> <article> <objectCategory> to   <article> <locationCategory>;
<transportplusCmd> = <transportAction> <article> <objectCategory> from <article> <locationCategory> to <article> <locationCategory>;

<getCmd> = 		<getAction> me <article> <objectCategory>;
<getPlusCmd> =  <getAction> me <article> <objectCategory> from <article> <locationCategory>;

<pointCmd> = 		 <pointAction> at <article> <objectCategory>;
<pointPlusCmd> = 	 <pointAction> at <article> <objectCategory> from <article> <locationCategory>;
<pointLocationCmd> = <pointAction> at <article> <locationCategory>;
<pointExitCmd> = 	 <pointAction> at an exit;

<findCmd> = 		<findAction> <article> <objectCategory>;
<findPlusCmd> = 	<findAction> <article> <objectCategory> from <article> <locationCategory>;
<findLocationCmd> = <findAction> <article> <locationCategory>;
<findExitCmd> = 	<findAction> an exit;

<navigateLocationCmd> = <navigateAction> to <article> <locationCategory>;
<navigateExitCmd> = 	<navigateAction> to an exit;

<transportAction> = transport | bring | carry;
<getAction> = 		bring | get | give;
<pointAction> = 	point | look;
<findAction> = 		detect | find;
<navigateAction> = 	go | move | navigate;

